    <string>Photos will be used to set profile picture in the app.</string>
    <key>NSRemindersUsageDescription</key>
    <string>App needs this permission to set reminder that will remind you about your fixed deposits, emis status.</string>
    <key>SchemeName</key>
    <string>$(SCHEME_NAME)</string>
    <key>UIAppFonts</key>
    <array>
        <string>light.ttf</string>
        <string>bold.ttf</string>
        <string>semibold.ttf</string>
        <string>medium.ttf</string>
        <string>regular.ttf</string>
        <string>ocrastd.otf</string>
    </array>
